{
  "name": "tsfp",
  "version": "0.1.1",
  "description": "A collection of functional utility libraries for JS projects",
  "main": "./lib/index.js",
  "typings": "./lib/index.d.ts",
  "typescript": {
    "definition": "./lib/index.d.ts"
  },
  "files": [
    "LICENSE",
    "lib"
  ],
  "scripts": {
    "clean": "npm cache clean && rimraf node_modules doc typings coverage dist",
    "clean:dist": "rimraf lib dist coverage",
    "lint": "./node_modules/.bin/tslint 'src/**/*.ts'",
    "test": "mocha src/**/*.ts --require ts-node/register --reporter dot test/**/*.spec.ts",
    "build": "ntsc",
    "prebuild": "npm run clean:dist && npm run typings-install && tsconfig",
    "watch": "npm run watch:build",
    "watch:test": "npm test -- --watch --watch-extensions ts --bail",
    "watch:build": "npm run build -- --watch",
    "typings-install": "typings install",
    "docs": "npm run docs:clean && npm run docs:build",
    "docs:clean": "rimraf doc",
    "docs:build": "typedoc --out doc --options typedoc.json --exclude '**/*.spec.ts' ./src/",
    "prepublish": "npm run build && babel dist/src --presets babel-preset-es2015 --out-dir lib && cp typings-ext.d.ts lib && npm run docs",
    "publishLocal": "npm pack"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zhenwenc/tsfp.git"
  },
  "keywords": [
    "tsfp",
    "typescript",
    "functional",
    "immutable"
  ],
  "author": "Zhenwen Cai <frederickcai@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zhenwenc/tsfp/issues"
  },
  "homepage": "https://github.com/zhenwenc/tsfp#readme",
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-core": "^6.6.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "chai": "^3.5.0",
    "immutable": "^3.7.6",
    "lodash": "^4.6.1",
    "ntypescript": "^1.201603270104.1",
    "should": "^8.2.2",
    "ts-node": "^0.7.1",
    "typedoc": "^0.3.12",
    "typescript": "^1.9.0-dev.20160327",
    "typings": "^0.7.9"
  },
  "dependencies": {
    "ntypescript": "1.201603270104.1"
  }
}
